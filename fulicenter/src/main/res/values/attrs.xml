<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Emojicon">
        <attr name="emojiconSize" format="dimension"/>
        <attr name="emojiconTextStart" format="integer"/>
        <attr name="emojiconTextLength" format="integer"/>
    </declare-styleable>
    <declare-styleable name="FlowIndicator">
        <attr name="count" format="integer"/>
        <attr name="r" format="dimension"/>
        <attr name="space" format="dimension"/>
        <attr name="normal_color" format="color|reference"/>
        <attr name="focus_color" format="color|reference"/>
        <attr name="focus" format="integer"/>
    </declare-styleable>
    <declare-styleable name="FocusView">
        <attr name="radius" format="dimension"/>
        <attr name="noamlaColor" format="color|reference"/>
        <attr name="focusColor" format="color|reference"/>
        <attr name="space_my" format="dimension"/>
        <attr name="focus_my" format="integer"/>
        <attr name="count_my" format="integer"/>
    </declare-styleable>
</resources>
